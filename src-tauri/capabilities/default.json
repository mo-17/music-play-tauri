{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for the music player app",
  "context": "default",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "core:path:default",
    "core:event:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-is-fullscreen"
  ]
}